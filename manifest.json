{
    "manifest_version": 3,
    "name": "Dharmatab",
    "version": "1.0",
    "description": "Displays a motivational quote on a relaxing background every time you open a new tab.",
    "author": "Giuseppe Pignataro",
    "chrome_url_overrides": {
      "newtab": "newtab.html"
    },
    "permissions": [],
    "icons": {
      "16": "icons/icon16.png",
      "32": "icons/icon32.png",
      "48": "icons/icon48.png",
      "64": "icons/icon64.png",
      "128": "icons/icon128.png"
    }
  }